        <item name="android:windowNoTitle">true</item>
    </style>
    <style
        name="Appwelcome"
        parent="android:Theme.Translucent.NoTitleBar.Fullscreen">
    </style>
    <style name="WelcomeStyle" parent="android:Theme">
        <item name="android:windowBackground">@drawable/bg_welcome</item>
        <item name="android:windowNoTitle">true</item>
